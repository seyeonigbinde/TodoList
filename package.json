{
  "name": "my-todolist-apps",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "migrateh": "heroku run knex migrate:latest -a my-todolist-apps",
    "rollbackh": "heroku run knex migrate:rollback -a my-todolist-apps",
    "databaseh": "heroku pg:psql -a my-todolist-apps",
    "seedh": "heroku run knex seed:run -a my-todolist-apps",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "deploy": "git push heroku main",
    "heroku-postbuild": "cd todolist-app && npm i && npm run build"
  },
  "engines": {
    "node": "14.16.0",
    "npm": "6.14.11"
  },
  "license": "ISC",
  "dependencies": {
    "babel-jest": "^26.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.95.7",
    "knex-cleaner": "1.3.1",
    "pg": "8.6.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "cross-env": "7.0.3",
    "eslint": "7.31.0",
    "nodemon": "2.0.12",
    "supertest": "6.1.4"
  }
}
